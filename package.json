{
  "name": "web-framework",
  "private": true,
  "license": "GPL-3.0",
  "workspaces": [
    "packages/*",
    "tools/*"
  ],
  "scripts": {
    "prepare": "husky install && husky add .husky/pre-commit \"lint-staged\" && husky add .husky/commit-msg \"commitlint --edit\"",
    "start": "lerna run start --scope @chenshaorui/web-framework-container",
    "build": "lerna run build"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-typescript": "8.2.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "commitlint": "12.1.4",
    "eslint": "7.26.0",
    "husky": "6.0.0",
    "lerna": "4.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.0",
    "rollup": "2.48.0",
    "rollup-plugin-copy": "^3.4.0",
    "tslib": "2.2.0",
    "typescript": "4.2.4"
  }
}
