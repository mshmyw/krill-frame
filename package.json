{
  "name": "web-framework",
  "private": true,
  "license": "GPL-3.0",
  "workspaces": [
    "packages/*",
    "tools/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap && rm -rf .husky && husky install && husky add .husky/pre-commit \"lint-staged\" && husky add .husky/commit-msg \"commitlint --edit\"",
    "start": "lerna run start --scope @chenshaorui/web-framework-container",
    "build": "lerna run build"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/runtime": "7.15.4",
    "@chenshaorui/web-framework-kernel": "^0.3.0",
    "@chenshaorui/web-framework-util": "^0.1.1",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.5",
    "@rollup/plugin-replace": "3.0.0",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "commitlint": "13.2.0",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "lerna": "4.0.0",
    "lint-staged": "11.1.2",
    "postcss": "8.3.8",
    "prettier": "2.4.1",
    "rollup": "2.58.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-postcss": "4.0.1",
    "rollup-plugin-typescript2": "0.30.0",
    "tslib": "2.3.1",
    "typescript": "4.4.3"
  }
