{
  "name": "web-framework",
  "private": true,
  "license": "GPL-3.0",
  "workspaces": [
    "packages/*",
    "tools/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap && rm -rf .husky && husky install && husky add .husky/pre-commit \"lint-staged\" && husky add .husky/commit-msg \"commitlint --edit\"",
    "start": "lerna run start --scope @chenshaorui/web-framework-container",
    "build": "lerna run build"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/runtime": "7.14.8",
    "@chenshaorui/web-framework-kernel": "^0.2.0",
    "@chenshaorui/web-framework-util": "^0.1.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.1",
    "@rollup/plugin-node-resolve": "13.0.2",
    "@rollup/plugin-replace": "3.0.0",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "commitlint": "12.1.4",
    "eslint": "7.31.0",
    "husky": "7.0.1",
    "lerna": "4.0.0",
    "lint-staged": "11.1.1",
    "prettier": "2.3.2",
    "rollup": "2.53.3",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-typescript2": "0.30.0",
    "tslib": "2.3.0",
    "typescript": "4.3.5"
  }
}
